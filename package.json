{
  "name": "react-cast",
  "version": "1.2.0",
  "description": "Create beautiful, animated grid-based layouts for react",
  "main": "lib/index.js",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack --config webpack-publish.config.js src/index.js dist/react-cast.js && NODE_ENV=production webpack --config webpack-publish.config.js src/index.js dist/react-cast.min.js",
    "lint": "eslint src",
    "test": "karma start",
    "prepublish": "npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmcmahen/react-cast.git"
  },
  "keywords": ["react", "reactjs", "grid", "animated", "layouts", "masonry"],
  "author": "Ben McMahen",
  "license": "MIT",
  "homepage": "https://github.com/bmcmahen/react-cast",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.8.0",
    "jquery": "^2.1.4",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.17",
    "react": "^15.6.1",
    "react-hot-loader": "^1.2.7",
    "rimraf": "^2.4.2",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "lodash.assign": "^3.2.0",
    "lodash.range": "^3.0.1",
    "prop-types": "^15.5.10",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0"
  }
}
